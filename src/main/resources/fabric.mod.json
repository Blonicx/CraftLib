{
	"schemaVersion": 1,
	"id": "craftlib",
	"version": "${version}",
	"name": "CraftLib",
	"description": "Minecraft Utility Library that makes development using Stonecutter easier.",
	"authors": [
		"Blonicx"
	],
	"contact": {
		"homepage": "https://modrinth.com/project/craftlib",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/craftlib/icon.png",
	"environment": "client",
	"entrypoints": {
		"client": [
			"dev.blonicx.craftlib.CraftLibClient"
		]
	},
	"mixins": [
		"craftlib.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.18.4",
		"minecraft": "~1.21",
		"java": ">=21",
		"fabric-api": "*"
	}
}